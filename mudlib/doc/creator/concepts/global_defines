Standard:/

NULL      0

TRUE      1
FALSE     0

TP        this_player()
TO        this_object()
TU        this_user()
PO        previous_object()

CAP(XXX)  capitalize(XXX)
ENV(XXX)  environment(XXX)
INV(XXX)  all_inventory(XXX)
ABS(XXX)  ((XXX < 0) ? (XXX * -1) : XXX)

HIM       query_objective()
HIS       query_possessive()
HE        query_pronoun()

